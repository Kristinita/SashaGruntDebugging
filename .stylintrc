{
    "blocks": {
        "expect": "never",
        "error": true
    },
    "brackets": {
        "expect": "never",
        "error": true
    },
    "colons": {
        "expect": "never",
        "error": true
    },
    "colors": {
        "expect": "always",
        "error": true
    },
    "commaSpace": {
        "expect": "always",
        "error": true
    },
    "commentSpace": {
        "expect": "always",
        "error": true
    },
    "cssLiteral": {
        "expect": "never",
        "error": true
    },
    "depthLimit": 4,
    "duplicates": {
        "expect": "always",
        "error": true
    },
    "efficient": {
        "expect": "always",
        "error": true
    },
    "exclude": [],
    "extendPref": "@extend",
    "globalDupe": {
        "expect": "always",
        "error": true
    },
    "groupOutputByFile": {
        "expect": "always",
        "error": true
    },
    "indentPref": false,
    "leadingZero": {
        "expect": "never",
        "error": true
    },
    "maxErrors": {
        "expect": "never",
        "error": true
    },
    "maxWarnings": {
        "expect": "never",
        "error": true
    },
    "mixed": {
        "expect": "always",
        "error": true
    },
    "namingConvention": false,
    "namingConventionStrict": {
        "expect": "always",
        "error": true
    },
    "none": {
        "expect": "never",
        "error": true
    },
    "noImportant": {
        "expect": "always",
        "error": true
    },
    "parenSpace": {
        "expect": "never",
        "error": true
    },
    "placeholder": {
        "expect": "always",
        "error": true
    },
    "prefixVarsWithDollar": false,
    "quotePref": {
        "expect": "double",
        "error": true
    },
    "semicolons": {
        "expect": "never",
        "error": true
    },
    "sortOrder": false,
    "stackedProperties": {
        "expect": "always",
        "error": true
    },
    "trailingWhitespace": {
        "expect": "never",
        "error": true
    },
    "universal": {
        "expect": "never",
        "error": true
    },
    "valid": false,
    "zeroUnits": {
        "expect": "never",
        "error": true
    },
    "zIndexNormalize": 100
}
